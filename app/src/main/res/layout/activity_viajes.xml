<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UT2.ut02_5_Viaje.Viajes">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:text="Tiempo"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajeTime2"
        app:layout_constraintEnd_toStartOf="@+id/viajeTime2"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/viajeTime2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:text="Ciudad de comienzo"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/comienzo"
        app:layout_constraintEnd_toStartOf="@+id/comienzo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/comienzo" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:layout_marginStart="4dp"
        android:text="Ciudad de destino"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/destino"
        app:layout_constraintEnd_toStartOf="@+id/destino"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/destino"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/destino"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        android:entries="@array/Ciudad"
        app:layout_constraintBottom_toTopOf="@+id/viajeDate2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.819"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comienzo"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/comienzo"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginEnd="4dp"
        android:entries="@array/Ciudad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025"
        tools:ignore="MissingConstraints" />

     <EditText
        android:id="@+id/viajeDate2"
        android:layout_width="167dp"
        android:layout_height="40dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="date"
        android:focusable="false"
        android:clickable="true"
        android:maxLines="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19" />

    <EditText
        android:id="@+id/viajeTime2"
        android:layout_width="165dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="time"
        android:focusable="false"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.819"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.291" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:text="fecha"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajeDate2"
        app:layout_constraintEnd_toStartOf="@+id/viajeDate2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/viajeDate2" />

    <ToggleButton
        android:id="@+id/viajeVulta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="88dp"
        android:text="ToggleButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viajeTime2" />

    <EditText
        android:id="@+id/vultaDate"
        android:layout_width="164dp"
        android:layout_height="41dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="date"
        android:focusable="false"
        android:clickable="true"
        android:maxLines="1"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/vultaTime"
        app:layout_constraintEnd_toStartOf="@+id/vultaTime"
        app:layout_constraintTop_toBottomOf="@+id/viajeVulta"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/vultaTime"
        android:layout_width="168dp"
        android:layout_height="41dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:ems="10"
        android:inputType="time"
        android:focusable="false"
        android:clickable="true"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viajeVulta"
        app:layout_constraintVertical_bias="0.113" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="119dp"
        android:layout_height="32dp"
        android:layout_marginEnd="64dp"
        android:text="ida y vulta"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajeVulta"
        app:layout_constraintEnd_toStartOf="@+id/viajeVulta"
        app:layout_constraintTop_toTopOf="@+id/viajeVulta" />

    <TextView
        android:id="@+id/vultaVD"
        android:layout_width="45dp"
        android:layout_height="20dp"
        android:layout_marginBottom="20dp"
        android:text="Fecha"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/vultaDate"
        app:layout_constraintEnd_toEndOf="@+id/vultaDate"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="@+id/vultaDate" />

    <TextView
        android:id="@+id/vultaVT"
        android:layout_width="60dp"
        android:layout_height="20dp"
        android:layout_marginBottom="20dp"
        android:text="Tiempo"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/vultaTime"
        app:layout_constraintEnd_toEndOf="@+id/vultaTime"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="@+id/vultaTime" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="413dp"
        android:layout_height="48dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp">

        <Button
            android:id="@+id/viajesENVIAR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Enviar" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="83dp"
        android:layout_height="31dp"
        android:layout_marginEnd="64dp"
        android:text="DNI"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajesDNI"
        app:layout_constraintEnd_toStartOf="@+id/viajesDNI"
        app:layout_constraintTop_toTopOf="@+id/viajesDNI"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/viajesDNI"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vultaTime"
        app:layout_constraintVertical_bias="0.588" />

    <EditText
        android:id="@+id/viajesNombre"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vultaTime"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="83dp"
        android:layout_height="31dp"
        android:layout_marginEnd="56dp"
        android:text="NOMBRE"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajesDNI"
        app:layout_constraintEnd_toStartOf="@+id/viajesNombre"
        app:layout_constraintTop_toTopOf="@+id/viajesNombre"
        app:layout_constraintVertical_bias="0.008" />

    <EditText
        android:id="@+id/viajesApellido"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vultaTime"
        app:layout_constraintVertical_bias="0.284" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_marginEnd="56dp"
        android:text="APELLIDO"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/viajesApellido"
        app:layout_constraintEnd_toStartOf="@+id/viajesApellido"
        app:layout_constraintTop_toTopOf="@+id/viajesApellido"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/visualError"
        android:layout_width="250dp"
        android:layout_height="38dp"
        android:layout_marginBottom="16dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
