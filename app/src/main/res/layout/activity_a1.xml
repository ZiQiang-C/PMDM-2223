<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UT2.ut02_6_EnvioVuelta.A1">

    <Button
        android:id="@+id/ut02_6_A1_bt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="ENVIARA2"
        app:layout_constraintBottom_toBottomOf="@+id/ut02_6_A1_nombre"
        app:layout_constraintEnd_toStartOf="@+id/ut02_6_A1_nombre"
        app:layout_constraintTop_toTopOf="@+id/ut02_6_A1_nombre"
